{
  "git": {
    "commitMessage": "release: v${version}",
    "requireBranch": "master",
    "tagName": "Release ${version}"
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "releaseNotes": "generate-release-notes.sh ${latestVersion} ${version}"
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md"
    }
  }
}